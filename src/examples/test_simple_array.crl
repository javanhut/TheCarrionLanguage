grim SimpleArray:
    init(elements):
        self.elements = elements
    
    spell append(val):
        self.elements = self.elements + [val]
        return None
    
    spell length():
        return len(self.elements)

arr = SimpleArray([1, 2, 3])
print("Created array with length:", arr.length())
arr.append(4)
print("After append, length:", arr.length())